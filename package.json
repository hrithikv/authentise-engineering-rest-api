{
  "name": "authentise-engineering-rest-api",
  "version": "1.0.0",
  "description": "Authentise Engineering Rest API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon -w ./src -x npm run dev:serve",
    "dev:serve": "babel-node src/index.js",
    "debug": "nodemon -w ./src -x npm run dev:debug",
    "dev:debug": "babel-node --nolazy --inspect-brk=9229 src/index.js",
    "format": "prettier --write './src/**/*.js'",
    "lint": "eslint './src/**/*.js' --quiet",
    "test": "jest",
    "build": "babel src -d dist --copy-files",
    "serve": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hrithikv/authentise-engineering-rest-api.git"
  },
  "keywords": [
    "nodejs",
    "rest",
    "api",
    "postgresql",
    "massivejs"
  ],
  "author": "Hrithik Vemparala",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "jest": "^22.4.0",
    "nodemon": "^1.15.0",
    "prettier": "^1.10.2",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.4",
    "db-migrate": "^0.10.5",
    "db-migrate-pg": "^0.3.1",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.1",
    "massive": "^4.6.6",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "superstruct": "^0.5.0"
  },
  "jest": {
    "forceExit": true
  }
}
